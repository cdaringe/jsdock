{
  "name": "jsdock",
  "version": "1.0.4",
  "description": "zero conf build & deploy of jsdoc api docs",
  "main": "src/index.js",
  "bin": "src/bin/cli.js",
  "scripts": {
    "test": "ava",
    "precommit": "run-p check lint test check-vulnerablities",
    "check": "counsel check",
    "check-vulnerablities": "nsp check",
    "lint": "standard",
    "docs:build": "src/bin/cli.js build",
    "docs:publish": "src/bin/cli.js publish",
    "postpublish": "run-s docs:build docs:publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cdaringe/jsdock.git"
  },
  "keywords": [
    "jsdoc",
    "no",
    "conf",
    "configuration",
    "api",
    "publish",
    "docs",
    "documentation"
  ],
  "author": "cdaringe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cdaringe/jsdock/issues"
  },
  "homepage": "https://github.com/cdaringe/jsdock#readme",
  "dependencies": {
    "counsel": "^0.5.2",
    "debug": "^3.0.1",
    "execa": "^0.10.0",
    "fs-extra": "^6.0.0",
    "jsdoc": "^3.5.5",
    "gh-pages": "^1.0.0",
    "resolve-jsdoc-bin": "^1.0.0",
    "minami": "^1.2.3",
    "perish": "^1.0.1",
    "pify": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.1",
    "nsp": "^3.1.0",
    "nyc": "^12.0.1",
    "sinon": "^6.0.1",
    "standard": "^11.0.1"
  }
}
